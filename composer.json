        "php": "^8.2",
        "kreait/firebase-php": "^7.9",
        "twig/twig": "^3.0",
        "tumblr/tumblr": "^0.4",
        "sentry/sdk": "^4",
        "lcobucci/jwt": "^5.2",
        "firebase/php-jwt": "^6.10",
        "symfony/mailer": "^7.0",
        "symfony/google-mailer": "^7.0",
        "slim/slim": "4.*",
        "openalpr/api": "*",
        "slim/twig-view": "^3.3"
    },
    "require-dev": {
        "sserbin/twig-linter": "@dev",
        "pdepend/pdepend": "^2.15",
        "phpmd/phpmd": "^2.13",
        "symfony/config": "^6.4"
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "artifact",
            "url": "openalpr/"
        }
